{
  "name": "languageserver-csharp",
  "main": "./lib/main",
  "version": "0.1.0",
  "description": "Provide C# language support via the Omnisharp Language Server",
  "repository": "https://github.com/atom/languageserver-csharp",
  "license": "MIT",
  "engines": {
    "atom": ">=1.17.0"
  },
  "dependencies": {
    "atom-languageclient": "git://github.com/atom/atom-languageclient.git",
    "omnisharp-client": "^7.0.6"
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "nuclide-datatip.provider": {
      "versions": {
        "0.0.0": "consumeDatatip"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "nuclide-outline-view": {
      "versions": {
        "0.0.0": "provideOutlines"
      }
    },
    "nuclide-definition-provider": {
      "versions": {
        "0.0.0": "provideDefinitions"
      }
    },
    "hyperclick.provider": {
      "versions": {
        "0.0.0": "provideHyperclick"
      }
    },
    "nuclide-find-references.provider": {
      "versions": {
        "0.0.0": "provideFindReferences"
      }
    }
  }
}
